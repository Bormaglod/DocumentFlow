                    },
                    {
                      "Name": "approved_date",
                      "DataType": "string"
                    }
                  ]
                },
                "Parameters": {
                  "$id": "rdc_sources_context_params",
                  "$values": [
                    "CalculationId"
                  ]
                },
                "SelectCommand": "select code, name, case status_id when 1002 then (select to_char(changed, 'DD.MM.YYYY') from history where reference_id = :CalculationId order by changed desc limit 1) else 'АРХИВ' end as approved_date from calculation where id = :CalculationId"
              },
              {
                "Name": "terminals",
                "Columns": {
                  "$id": "rdc_sources_terminals",
                  "$values": [
                    {
                      "Name": "name",
                      "DataType": "string"
                    },
                    {
                      "Name": "amount",
                      "DataType": "decimal"
                    }
                  ]
                },
                "Parameters": {
                  "$id": "rdc_sources_terminals_params",
                  "$values": [
                    "CalculationId"
                  ]
                },
                "SelectCommand": "with recursive tree_goods as (select id, parent_id, name from goods where parent_id = '17f8df70-2ad6-4d97-95db-6425cf3249f0' union all select g.id, g.parent_id, g.name from goods g join tree_goods on (tree_goods.id = g.parent_id)) select g.name, cig.amount from calc_item_goods cig join tree_goods g on (g.id = cig.item_id) where cig.owner_id = :CalculationId order by g.name"
              },
              {
                "Name": "blocks",
                "Columns": {
                  "$id": "rdc_sources_blocks",
                  "$values": [
                    {
                      "Name": "name",
                      "DataType": "string"
                    },
                    {
                      "Name": "amount",
                      "DataType": "decimal"
                    }
                  ]
                },
                "Parameters": {
                  "$id": "rdc_sources_blocks_params",
                  "$values": [
                    "CalculationId"
                  ]
                },
                "SelectCommand": "with recursive tree_goods as (select id, parent_id, name from goods where parent_id = '2d34202e-159d-467b-9e97-5a0bb5494fa4' union all select g.id, g.parent_id, g.name from goods g join tree_goods on (tree_goods.id = g.parent_id)) select g.name, cig.amount from calc_item_goods cig join tree_goods g on (g.id = cig.item_id) where cig.owner_id = :CalculationId order by g.name"
              },
              {
                "Name": "tube",
                "Columns": {
                  "$id": "rdc_sources_tube",
                  "$values": [
                    {
                      "Name": "name",
                      "DataType": "string"
                    },
