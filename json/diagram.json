{
  "name": "diagram",
  "viewer": {
    "dataset": {
      "select": "select id, name from transition"
    },
    "data-type": "Directory",
    "columns": [
      {
        "datafield": "id",
        "text": "Id",
        "type": "Text",
        "width": 180,
        "visible": false
      },
      {
        "datafield": "name",
        "text": "Наименование",
        "type": "Text",
        "auto-size": true
      }
    ],
    "command-groups": [
      {
        "name": "Схема",
        "commands": [
          {
            "name": "ViewDiagramSchema",
            "method": "Embedded",
            "command": "open-diagram",
            "show-title": true
          }
        ]
      }
    ],
    "command-visible": [ "refresh", "open-diagram" ],
    "double-click-command": "open-diagram",
    "commandbar-visible": false,
    "sorts": [
      {
        "name": "name"
      }
    ]
  }
}